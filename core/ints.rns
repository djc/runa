from libc.stdio import sprintf

class byte:
	pass

class i32:
	pass

class u32:
	def __str__(self) -> $str:
		data = __malloc__(20)
		len = sprintf(data, '%u\0'.data, self)
		return str(len as u32, data)

class word:
	
	def __bool__(self) -> bool:
		return self != 0
	
	def __str__(self) -> $str:
		data = __malloc__(20)
		len = sprintf(data, '%ld\0'.data, self)
		return str(len as u32, data)

class uword:
	pass
