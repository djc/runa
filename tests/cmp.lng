def main(name: str, args: array[str]):
	
	print('bool')
	print(True)
	print(False)
	
	print('eq, int/bool')
	print(1 == 3)
	print(2 == 2)
	print(True == True)
	print(False == True)
	
	print('eq, str')
	print("a" == "b")
	print("aa" == "b")
	print("aa" == "bb")
	print("a" == "a")
	
	print('ne')
	print(1 != 4)
	print(False != False)
	print("a" != "b")
	
	print('lt, int')
	print(1 < 4)
	print(2 < 2)
	print(5 < 4)
	
	print('lt, str')
	print('a' < 'aa')
	print('a' < 'b')
	print('a' < 'a')
	print('b' < 'a')

